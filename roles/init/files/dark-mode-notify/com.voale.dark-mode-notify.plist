<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.voale.dark-mode-notify</string>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/Users/voale/dark-mode-notify-stderr.log</string>
    <key>StandardOutPath</key>
    <string>/Users/voale/dark-mode-notify-stdout.log</string>
    <key>ProgramArguments</key>
    <array>
       <string>/Users/voale/bin/dark-mode-notify</string>
       <string>/bin/zsh</string>
       <string>-c</string>
       <string>/Users/voale/bin/theme</string>
    </array>
</dict>
</plist>
